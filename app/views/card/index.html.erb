		<script type="text/javascript">
			$('.uinfo_box').fancybox({
				'closeBtn' : false,
				'width' : 300,
				topRatio : 0.2,
				closeEffect : 'fade',
				helpers : {
					overlay : null
				}
			});

			$('.pay_box').fancybox({
				'closeBtn' : false,
				'width' : 250,
				'topRatio' : 0.2,
				'clickClose' : true,
				openEffect : 'fade',
				helpers : {
					overlay : null
				}

			});

			function validate_code() {
				var code = $.trim($("#pay_code").val());
				var params = "code=" + code;
				$.ajax({
					type : "POST",
					url : "/homepage/validate_code",
					data : params,
					dataType : "json",
					success : function(data) {
						var msg = "充值成功，本次充值" + data.point + "点";
						var val = data.card_credit;
						$("#code_message").html(msg);
						$("#credit_div").html(val);
					},
					error : function(xhr) {
						//alert(data.errors);
						//var errors = $.parseJSON(xhr.responseText).errors
						$("#code_message").html("兑换号无效，或已被使用");
					}
				});
			}

			function clearPaybox() {
				$.fancybox.close();
				$("#code_message").html("");
			}

		</script>

