				<%= javascript_include_tag "leftmenu.js" %>

				<script type="text/javascript">
					/*s
					 *  Simple image gallery. Uses default settings
					 */

					var currentpage = "agency";
					var user_name = $.trim($("#session_user").text());
					var options = {
						color : "#FFFFFF"
					};

					function showpage(id, address) {
						$("#menu_" + id).addClass("selected");
						if (currentpage != id) {
							$("#menu_" + currentpage).removeClass("selected");
						}
						$('#page').html("<iframe src='" + address + "' width='810' frameborder='0' scrolling='no' height='655'>我們使用了框架技术，但是您的浏览器不支持框架，请升级您的浏览器以便正常访问脚本之家。 </iframe>");
						$('#page').show();
						currentpage = id;

					}

				</script>
